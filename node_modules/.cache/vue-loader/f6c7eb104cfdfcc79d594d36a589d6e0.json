      todos:[],\n          title: '',\n          important: false\n      }\n  } ,\n  created: function(){\n      console.log(this.todos);\n  } ,\n  methods:{\n      addTodo(){\n          if(!this.title){\n              alert(\"TODOに何も書いてません\")\n          }\n          else{\n              var new_todo = {title: this.title, important: this.important, completed: false}\n              this.todos.push(new_todo);\n              this.todos = this.todos.filter(todo => !todo.completed)\n          }\n      }\n  }\n}\n</script>\n\n<style scoped>\n</style>"]}]}